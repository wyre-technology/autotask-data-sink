          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  r.first_name || ' ' || r.last_name as \"Resource\",\n  COUNT(t.id) as \"Open Tickets\",\n  AVG(EXTRACT(EPOCH FROM (NOW() - t.created_at))/3600)::NUMERIC(10,2) as \"Avg Age (hours)\"\nFROM\n  autotask.tickets t\nJOIN\n  autotask.resources r ON t.assigned_resource_id = r.id\nWHERE\n  t.status != 5 -- Assuming 5 is 'Completed'\nGROUP BY\n  r.id, r.first_name, r.last_name\nORDER BY\n  \"Open Tickets\" DESC",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "id"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "tickets",
          "timeColumn": "created_at",
          "timeColumnType": "timestamp",
          "where": [
            {
